msgstr "Til innlogginssiden"

#: lostpassword/templates/resetpassword.php:8
msgid "New password"
msgstr "Nytt passord"

#: lostpassword/templates/resetpassword.php:11
msgid "Reset password"
msgstr "Tilbakestill passord"

