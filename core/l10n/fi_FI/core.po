msgid "To login page"
msgstr "Kirjautumissivulle"

#: lostpassword/templates/resetpassword.php:8
msgid "New password"
msgstr "Uusi salasana"

#: lostpassword/templates/resetpassword.php:11
msgid "Reset password"
msgstr "Palauta salasana"

